  "dependencies": {
    "@firebase/app": "^0.1.4",
    "@firebase/auth": "^0.3.0",
    "@firebase/firestore": "^0.2.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "classnames": "^2.2.5",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "material-ui": "1.0.0-beta.24",
